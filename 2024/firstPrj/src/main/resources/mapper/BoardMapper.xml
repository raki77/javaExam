<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sejong.firstPrj.dao.BoardMapper">
    <select id="getAllBoardList" resultType="com.sejong.firstPrj.dto.Board">
        SELECT image_id
            , image_name
            , file_path
            , uploaded_at
        FROM
            Images;
    </select>

</mapper>